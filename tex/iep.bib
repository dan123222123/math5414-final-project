  copyright = {Copyright (c) 2022 Kanae Akaiwa, Akira Yoshida, Koichi Kondo},
  langid = {english},
  keywords = {Band matrix,Discrete integrable system,Inverse eigenvalue problem,Oscillatory matrix},
  
}

